{
  "expo": {
    "name": "Bienvenir",
    "slug": "bienvenir",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bienvenir_icon.png",
    "scheme": "celodappkit",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Bienvenir platform",
    "githubUrl": "https://github.com/nestorbonilla/bienvenir"
  }
}
